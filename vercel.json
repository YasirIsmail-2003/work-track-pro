{
  "version": 2,
  "builds": [
    {
      "src": "server/package.json",
      "use": "@vercel/next",
      "config": {}
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/server/(.*)",
      "dest": "/server/$1"
    }
  ]
}
